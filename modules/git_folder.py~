import urllib2
import re

# Author: chilimannen.
# 2015-07-27

def scan(url):
	""" Determines if a .git folder is hosted on given URL by http code
	and the start tag of the configuration file. """
	success = False
	try:
		request = urllib2.urlopen(url + '.git/config', timeout=1)
		if request.getcode() == 200:	
			html = request.read()
			if html.startswith('[core]'):
				success = True
	except:
		pass
	return success
